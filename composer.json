{
  "name": "kunstmaan/google-api",
  "type": "library",
  "description": "Kunstmaan Google API is an SDK for google apis",
  "license": "MIT",
  "authors": [
    {
      "name": "Steven Poelmans",
      "email": "steven.poelmans@kunstmaan.be"
    }
  ],
  "require": {
    "php": ">=7.0",
    "guzzlehttp/guzzle": "^6.2.2",
    "guzzlehttp/psr7": "^1.3.1",
    "guzzlehttp/promises": "^1.0"
  },
  "require-dev": {
